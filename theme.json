{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"customTemplates": [
		{
			"name": "hidden-title",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Hidden Title"
		}
	],
	"settings": {
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#344334",
						"#f9f9f8"
					],
					"name": "Evergreen",
					"slug": "evergreen"
				},
				{
					"colors": [
						"#943131",
						"#f9f9f8"
					],
					"name": "Earth",
					"slug": "earth"
				},
				{
					"colors": [
						"#000000",
						"#7F7F7F"
					],
					"name": "Dark Grayscale",
					"slug": "dark-grayscale"
				},
				{
					"colors": [
						"#000000",
						"#ffffff"
					],
					"name": "Grayscale",
					"slug": "grayscale"
				}
			],
			"palette": [
				{
					"color": "#344334",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#f9f9f8",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#943131",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#798f79",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#eceeeb",
					"name": "Shadow",
					"slug": "shadow"
				},
				{
					"color": "#d0d4d0",
					"name": "Outline",
					"slug": "outline"
				}
			]
		},
		"custom": {
			"radius": "0.5rem"
		},
		"layout": {
			"contentSize": "40rem",
			"wideSize": "64rem"
		},
		"spacing": {
			"blockGap": true,
			"spacingScale": {
				"steps": 7
			},
			"spacingSizes": [
				{
					"name": "1",
					"size": "min(20px, 1vw)",
					"slug": "10"
				},
				{
					"name": "2",
					"size": "min(30px, 2vw)",
					"slug": "20"
				},
				{
					"name": "3",
					"size": "min(40px, 4vw)",
					"slug": "30"
				},
				{
					"name": "4",
					"size": "min(50px, 6vw)",
					"slug": "40"
				},
				{
					"name": "5",
					"size": "min(60px, 10vw)",
					"slug": "50"
				},
				{
					"name": "6",
					"size": "min(70px, 12vw)",
					"slug": "60"
				},
				{
					"name": "7",
					"size": "min(80px, 14vw)",
					"slug": "70"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Newsreader,ui-serif,serif",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"fontFamily": "Inter,ui-sans-serif,sans-serif",
					"name": "Alternate",
					"slug": "alternate"
				}
			],
			"fontSizes": [
				{
					"name": "X-Small",
					"size": "clamp(0.75rem, 0.75vw, 1rem)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(0.75rem, 0.926vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.125rem, 1.4vw, 1.5rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, 2vw, 2rem)",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "clamp(1.375rem, 3.5vw, 2.5rem)",
					"slug": "x-large"
				},
				{
					"name": "XX-Large",
					"size": "clamp(1.875rem, 8vw, 5.5rem)",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/code": {
				"border": {
					"color": "var(--wp--preset--color--outline)",
					"radius": "var(--wp--custom--radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--shadow)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"core/comment-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--alternate)"
				}
			},
			"core/comments-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--alternate)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				}
			},
			"core/post-comments-form": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontStyle": "italic"
				}
			},
			"core/post-template": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/search": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"width": "0"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--outline)"
				}
			},
			"core/site-logo": {
				"border": {
					"color": "var(--wp--preset--color--outline)",
					"style": "solid",
					"width": "1px"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--alternate)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"letterSpacing": "0.2em",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"letterSpacing": "0.05em"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "normal",
					"letterSpacing": "-0.0125em",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "normal"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "normal",
					"letterSpacing": "0.1em",
					"textTransform": "uppercase"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "normal"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "bold"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"link": {
				":focus": {
					"border": {
						"radius": "var(--wp--custom--radius)"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--40)",
			"padding": {
				"left": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.66"
		}
	},
	"templateParts": [
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "uncategorized",
			"name": "post-navigation-stacked",
			"title": "Post Navigation Stacked"
		}
	],
	"version": 2
}